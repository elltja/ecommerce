    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.515.0",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "next-intl": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.53.0",
    "@tailwindcss/postcss": "^4.0.15",
